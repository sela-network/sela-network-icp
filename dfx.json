{
  "canisters": {
    "backend": {
      "main": "src/backend/main.mo",
      "type": "motoko",
      "http": {
        "port": 4943
      },
      "declarations": {
        "output": "src/declarations/backend",
        "node_compatibility": true
      }
    },
    "service_polling": {
      "main": "src/service_polling/polling.mo",
      "type": "motoko",
      "http": {
        "port": 4943
      },
      "declarations": {
        "output": "src/declarations/service_polling",
        "node_compatibility": true
      }
    },
    "newCanister": {
      "main": "src/newCanister/newCanister.mo",
      "type": "motoko",
      "http": {
        "port": 4943
      },
      "declarations": {
        "output": "src/declarations/newCanister",
        "node_compatibility": true
      }
    },
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "frontend": {}
    },
    "dashboard": {
      "dependencies": ["backend", "internet_identity"],
      "frontend": {
        "entrypoint": "src/dashboard/index.html"
      },
      "source": ["src/dashboard/assets", "dist/"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "version": 1
}
