{
  "canisters": {
    "rpcCanister": {
      "main": "src/rpcCanister/main.mo",
      "type": "motoko",
      "environment": {
        "RPC_CANISTER_ID": "process.env.RPC_CANISTER_ID"
      },
      "http": {
        "port": 4943
      },
      "declarations": {
        "output": "src/declarations/rpcCanister",
        "node_compatibility": true
      }
    },
    "backend": {
      "main": "src/backend/main.mo",
      "type": "motoko",
      "http": {
        "port": 4943
      },
      "declarations": {
        "output": "src/declarations/backend",
        "node_compatibility": true
      }
    },
    "main": {
      "main": "src/nodeCanister/main.mo",
      "type": "motoko",
      "http": {
        "port": 4943
      },
      "declarations": {
        "output": "src/declarations/main",
        "node_compatibility": true
      }
    },
    "sock": {
      "main": "src/nodeCanister/sock.mo",
      "type": "motoko",
      "http": {
        "port": 4943
      },
      "declarations": {
        "output": "src/declarations/sock",
        "node_compatibility": true
      }
    },
    "webSocketCanister": {
      "main": "src/nodeCanister/canister.mo",
      "type": "motoko",
      "http": {
        "port": 4943
      },
      "declarations": {
        "output": "src/declarations/canister",
        "node_compatibility": true
      }
    },
    "lib": {
      "main": "src/nodeCanister/lib.mo",
      "type": "motoko",
      "http": {
        "port": 4943
      },
      "declarations": {
        "output": "src/declarations/lib",
        "node_compatibility": true
      }
    },
    "db": {
      "main": "src/nodeCanister/db.mo",
      "type": "motoko",
      "http": {
        "port": 4943
      },
      "declarations": {
        "output": "src/declarations/db",
        "node_compatibility": true
      }
    },
    "internetIdentity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "frontend": {}
    },
    "dashboard": {
      "dependencies": ["main", "internetIdentity"],
      "frontend": {
        "entrypoint": "src/dashboard/index.html"
      },
      "source": ["src/dashboard/assets", "dist/"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "version": 1
}
